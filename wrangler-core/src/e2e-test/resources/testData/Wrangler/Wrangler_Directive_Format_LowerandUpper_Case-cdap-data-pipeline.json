        "postActions": [],
        "properties": {},
        "processTimingEnabled": true,
        "stageLoggingEnabled": true,
        "stages": [
            {
                "name": "BigQueryTable",
                "plugin": {
                    "name": "BigQueryTable",
                    "type": "batchsource",
                    "label": "BigQueryTable",
                    "artifact": {
                        "name": "google-cloud",
                        "version": "0.22.0-SNAPSHOT",
                        "scope": "SYSTEM"
                    },
                    "properties": {
                        "useConnection": "true",
                        "connection": "${conn(test_1)}",
                        "dataset": "test_automation",
                        "table": "Accounts",
                        "schema": "{\"type\":\"record\",\"name\":\"output\",\"fields\":[{\"name\":\"ID\",\"type\":[\"long\",\"null\"]},{\"name\":\"name\",\"type\":[\"string\",\"null\"]},{\"name\":\"email\",\"type\":[\"string\",\"null\"]}]}",
                        "enableQueryingViews": "false"
                    }
                },
                "outputSchema": "{\"type\":\"record\",\"name\":\"output\",\"fields\":[{\"name\":\"ID\",\"type\":[\"long\",\"null\"]},{\"name\":\"name\",\"type\":[\"string\",\"null\"]},{\"name\":\"email\",\"type\":[\"string\",\"null\"]}]}",
