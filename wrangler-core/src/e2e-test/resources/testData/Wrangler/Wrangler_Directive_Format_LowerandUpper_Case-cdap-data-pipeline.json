                        "suffix": "yyyy-MM-dd-HH-mm",
                        "format": "csv",
                        "location": "us",
                        "contentType": "application/octet-stream",
                        "schema": "{\"type\":\"record\",\"name\":\"record\",\"fields\":[{\"name\":\"ID\",\"type\":[\"long\",\"null\"]},{\"name\":\"name\",\"type\":[\"string\",\"null\"]},{\"name\":\"email\",\"type\":[\"string\",\"null\"]}]}",
