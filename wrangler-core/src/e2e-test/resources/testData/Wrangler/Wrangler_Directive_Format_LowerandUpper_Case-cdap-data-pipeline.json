            {
                "name": "GCS",
                "plugin": {
                    "name": "GCS",
                    "type": "batchsink",
                    "label": "GCS",
                    "artifact": {
                        "name": "google-cloud",
                        "version": "0.22.0-SNAPSHOT",
                        "scope": "SYSTEM"
                    },
                    "properties": {
                        "useConnection": "false",
                        "project": "cdf-athena",
                        "serviceAccountType": "filePath",
