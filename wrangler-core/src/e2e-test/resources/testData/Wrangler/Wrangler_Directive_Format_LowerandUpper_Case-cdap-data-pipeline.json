                        "name": "wrangler-transform",
                        "version": "4.9.0-SNAPSHOT",
                        "scope": "SYSTEM"
                    },
                    "properties": {
                        "directives": "uppercase :name\nlowercase :email",
                        "field": "*",
                        "precondition": "false",
                        "workspaceId": "1e874b06-fb23-4fbb-9600-0b052c5515d3",
                        "schema": "{\"type\":\"record\",\"name\":\"record\",\"fields\":[{\"name\":\"ID\",\"type\":[\"long\",\"null\"]},{\"name\":\"name\",\"type\":[\"string\",\"null\"]},{\"name\":\"email\",\"type\":[\"string\",\"null\"]}]}",
